extend_with rest

assertPostContains FAILED http://localhost:8080/create/me/myid.json
assertPostContains OK http://localhost:8080/create/me/myid.json source=obiwonn@gmail.com
assertPostContains FAILED http://localhost:8080/create/me/myid.json source=obiwonn@gmail.com

assertPostContains FAILED http://localhost:8080/entry.json url=http://python.org
assertPostContains OK http://localhost:8080/entry.json url=http://python.org&tags=python

assertPostContains FAILED http://localhost:8080/tag.json url=http://python.org&tags=python
assertPostContains OK http://localhost:8080/tag.json name=python

assertGetNotContains python http://localhost:8080/search.json
assertGetContains python http://localhost:8080/search.json?q=python

assertGetContains python http://localhost:8080/suggest/tags?url=http://python.org
assertGetNotContains python http://localhost:8080/suggest/tags

assertGetContains python http://localhost:8080/suggest/tag?q=py
assertGetNotContains python http://localhost:8080/suggest/tag

assertGetContains python http://localhost:8080/suggest/search?q=py
assertGetNotContains python http://localhost:8080/suggest/search

assertGetContains python http://localhost:8080/tags.json

assertGetContains FAILED http://localhost:8080/myid2/.json?q=python
assertNotContains python http://localhost:8080/.json?q=python
assertGetContains python http://localhost:8080/myid/.json?q=python

assertPostContains OK http://localhost:8080/delete/me/myid.json
assertPostContains FAILED http://localhost:8080/delete/me/myid.json

assertPostContains OK http://localhost:8080/resettags

